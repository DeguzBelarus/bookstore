{
   "name": "api",
   "version": "1.0.0",
   "private": true,
   "scripts": {
      "start": "node ./bin/www",
      "server": "nodemon ./bin/www",
      "client": "yarn --cwd ../front run start",
      "client:install": "yarn --cwd front add",
      "client:build": "yarn --cwd front run build",
      "dev": "concurrently \"yarn server\" \"yarn client\""
   },
   "dependencies": {
      "config": "^3.3.7",
      "cross-env": "^7.0.3",
      "dotenv": "~6.0.0",
      "express": "~4.16.0",
      "express-joi-validation": "~0.3.0",
      "joi": "~13.7.0",
      "morgan": "~1.9.1",
      "react-scripts": "^5.0.0",
      "swagger-ui-express": "^4.3.0"
   },
   "devDependencies": {
      "concurrently": "^7.0.0",
      "nodemon": "^2.0.15"
   }
}
